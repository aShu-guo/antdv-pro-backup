{
  "name": "antdv-pro",
  "type": "module",
  "version": "0.0.19",
  "packageManager": "pnpm@8.4.0",
  "author": "aibayanyu <aibayanyu@qq.com>",
  "repository": "antdv-pro/antdv-pro",
  "scripts": {
    "dev:admin": "vite --mode development",
    "dev": "run-p dev:admin dev:mockServer",
    "dev:mock": "pnpm -F servers dev",
    "dev:mockServer": "esno ./scripts/mock-server.ts",
    "build:test": "vue-tsc && vite build --mode test",
    "build:prod": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html}\"",
    "bump:patch": "changelogen --bump --output CHANGELOG.md --release --patch",
    "bump:minor": "changelogen --bump --output CHANGELOG.md --release --minor",
    "bump:major": "changelogen --bump --output CHANGELOG.md --release --major",
    "prepare": "husky install",
    "dir-tree": "esno ./scripts/dir-tree",
    "gen:uno": "esno ./scripts/gen-unocss",
    "toJS": "esno scripts/to-js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@ctrl/tinycolor": "^3.6.1",
    "@v-c/utils": "^0.0.23",
    "@vueuse/core": "^10.4.1",
    "ant-design-vue": "^4.0.2",
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "9.3.0-beta.26",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.12",
    "@types/treeify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "antdv-component-resolver": "^1.0.4",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
    "directory-tree": "^3.5.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.16.3",
    "execa": "^7.2.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "treeify": "^1.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unocss": "^0.51.13",
    "unocss-preset-chinese": "^0.0.7",
    "unocss-preset-ease": "^0.0.3",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^0.7.38",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
